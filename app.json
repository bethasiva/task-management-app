{
  "expo": {
    "name": "Todo",
    "slug": "task-management-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icons/icon.png",
    "scheme": "taskapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.factscub.taskmanagementapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icons/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.factscub.taskmanagementapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icons/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icons/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4966a7c5-4a08-43b8-9acd-e9529e47a807"
      }
    }
  }
}
